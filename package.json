{
  "name": "messenger-brain-sender-api",
  "version": "1.0.0",
  "description": "Messenger Brain Sender API - High-performance WhatsApp messaging API with Sequelize ORM",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --clear --ignore-watch node_modules --watch src src/index.ts",
    "dev:clean": "node scripts/kill-port.js 9000 && npm run dev",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:setup",
    "migration:generate": "sequelize-cli migration:generate --name",
    "model:generate": "sequelize-cli model:generate --name",
    "seed:generate": "sequelize-cli seed:generate --name",
    "setup": "npm run build && npm run db:setup"
  },
  "keywords": [
    "messenger-brain",
    "whatsapp",
    "api",
    "messaging",
    "typescript",
    "node",
    "sequelize",
    "high-performance"
  ],
  "author": "Messenger Brain Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/bull": "^4.10.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.9.0",
    "@types/redis": "^4.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.4",
    "puppeteer": "^24.26.0",
    "redis": "^5.8.2",
    "resend": "^6.3.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sharp": "^0.34.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
